#include <stdio.h>

int main() {
    int i, j;
    char ch = 'a';

    for (i = 1; i <= 4; i++) {
        for (j = 1; j <= i; j++) {
            printf("%c ", ch);
            ch++;
            if (ch > 'd') {
                ch = 'a';
            }
        }
        printf("\n");
    }

    return 0;
}
